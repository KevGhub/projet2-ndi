<h2>Requête </h2>

<ul>
  {{#each reqDoc}}
  <li>
    <h4>{{this.category}}</h4>
    <h3>{{this.title}}</h3>
    <br>
    <p>{{this.goal}}</p>
    <br>
    <p>{{this.description}}</p>
    <br>
    <p>{{this.place}}</p>
    <br>
    <img src="{{this.img}}" alt="Requête photo">
  </li>
  {{/each}}
</ul>
<a href="/liste-requetes">Retour aux requêtes</a>