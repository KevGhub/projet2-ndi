<h3>Requêtes {{requeteResults.category.enumValues}}</h3>

<ul>
  {{#each requeteResults.category}}
  <li>
    <h3>{{this.title}}</h3>
    <br>
    <p>{{this.goal}}</p>
    <br>
    <img src="{{this.img}}" alt="Requête photo">
    <br>
    <a href="/requete-detail/{{this._id}}">Voir la requête</a>


  </li>
  {{/each}}
</ul>

<hr>
<br>
<a href="/nouvelle-requete">Créer une requête</a>